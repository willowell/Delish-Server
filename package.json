{
  "name": "mealdb-apollo-server",
  "description": "An Apollo GraphQL Server for the TheMealDB's API",
  "keywords": [
    "apollo",
    "apollo-server",
    "rest",
    "thedatadb",
    "themealdb",
    "food",
    "api",
    "graphql",
    "gql"
  ],
  "version": "1.1.0",
  "author": "William Howell <wlm.howell@gmail.com>",
  "repository": {
    "url": "https://github.com/willowell/Delish-Server"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./build",
    "clean-all": "yarn clean && yarn bundle-clean",
    "lint": "npx eslint .",
    "plzfixmycode": "npx eslint . --fix",
    "gen": "yarn graphql-codegen",
    "bundle-clean": "rimraf ./lambda/delish-api/bundle && rimraf ./lambda/delish-api/graphql",
    "bundle-src": "cpx ./src/**/**/*.* lambda/delish-api/bundle",
    "bundle-graphql": "cpx ./graphql/**/*.* lambda/delish-api/graphql",
    "start": "yarn build && node --require dotenv/config build/index.js",
    "start-lambda": "netlify-lambda serve lambda/delish-api",
    "start-dev": "NODE_ENV=development nodemon .",
    "build": "rimraf ./build && tsc",
    "build-lambda": "yarn bundle-clean; yarn bundle-src; yarn bundle-graphql; netlify-lambda build lambda/delish-api"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@graphql-codegen/cli": "^1.19.0",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-resolvers": "^1.17.10",
    "@types/dotenv": "^8.2.0",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.14.5",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "cpx": "^1.5.0",
    "eslint": "^7.12.1",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "netlify-lambda": "^2.0.1",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.9.4",
    "apollo-server": "^2.18.2",
    "apollo-server-lambda": "^2.18.2",
    "dotenv": "^8.2.0",
    "graphql": "^15.4.0",
    "lodash": "^4.17.21"
  }
}
