{
  "name": "mealdb-apollo-server",
  "description": "An Apollo GraphQL Server for the TheMealDB's API",
  "keywords": [
    "apollo",
    "apollo-server",
    "rest",
    "thedatadb",
    "themealdb",
    "food",
    "api",
    "graphql",
    "gql"
  ],
  "version": "1.0.0",
  "author": "William Howell <wlm.howell@gmail.com>",
  "repository": {
    "url": "https://github.com/willowell/Recipe-Box-Server"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start": "yarn build && node --require dotenv/config build/index.js",
    "start:dev": "NODE_ENV=development nodemon .",
    "lint": "npx eslint .",
    "plzfixmycode": "npx eslint . --fix",
    "gen": "yarn graphql-codegen"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.15.3",
    "@graphql-codegen/typescript": "^1.15.3",
    "@graphql-codegen/typescript-resolvers": "^1.15.3",
    "@types/dotenv": "^8.2.0",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.9.2",
    "apollo-server": "^2.15.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.1.0",
    "lodash": "^4.17.15"
  }
}
